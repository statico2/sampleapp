{
  "name": "@sampleapp/backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon -e ts,js,graphql,json -w . -w ../schema.graphql -x ../node_modules/.bin/ts-node src/server.ts",
    "migrate:make": "knex --knexfile knexfile.ts -x ts migrate:make",
    "migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "seed:make": "knex --knexfile knexfile.ts seed:make",
    "seed:run": "knex --knexfile knexfile.ts seed:run"
  },
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.11",
    "pg": "^7.18.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/knex": "^0.16.1",
    "@types/pg": "^7.14.3",
    "@types/uuid": "^7.0.2"
  }
}
